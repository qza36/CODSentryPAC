Panels:
  - Class: rviz_common/Displays
    Name: Displays
  - Class: rviz_common/Views
    Name: Views
Visualization Manager:
  Displays:
    - Class: rviz_default_plugins/Grid
      Name: Grid
      Value: true
      Cell Size: 1
      Line Style:
        Value: Lines
      Plane Cell Count: 100

    - Class: rviz_default_plugins/Marker
      Name: FinalPath
      Topic:
        Value: /final_path_vis
      Value: true

    - Class: rviz_default_plugins/Marker
      Name: FinalLine
      Topic:
        Value: /final_line_vis
      Value: true

    - Class: rviz_default_plugins/Marker
      Name: ReferencePath
      Topic:
        Value: /reference_path_vis
      Value: true

    - Class: rviz_default_plugins/Marker
      Name: CurPosition
      Topic:
        Value: /cur_position_vis
      Value: true

    - Class: rviz_default_plugins/Marker
      Name: TargetPosition
      Topic:
        Value: /target_vis
      Value: true

  Global Options:
    Fixed Frame: world
  Tools:
    - Class: rviz_default_plugins/MoveCamera
    - Class: rviz_default_plugins/Select
    - Class: rviz_default_plugins/SetGoal
      Topic: /goal_pose
  Value: true
  Views:
    Current:
      Class: rviz_default_plugins/TopDownOrtho
      Name: TopDownOrtho
      Scale: 20
      X: 0
      Y: 0
Window Geometry:
  Width: 1200
  Height: 800
